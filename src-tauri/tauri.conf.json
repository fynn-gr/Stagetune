{
	"$schema": "..\\node_modules/@tauri-apps/cli\\schema.json",
	"productName": "Stagetune",
	"version": "1.0.0",
	"identifier": "com.fynn.gr",
	"build": {
		"beforeBuildCommand": "npm run build",
		"beforeDevCommand": "npm run dev",
		"frontendDist": "../dist",
		"devUrl": "http://localhost:1420"
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"windows": {
			"certificateThumbprint": null,
			"digestAlgorithm": "sha256",
			"timestampUrl": ""
		},
		"longDescription": "",
		"macOS": {
			"entitlements": null,
			"minimumSystemVersion": "11.0",
			"exceptionDomain": "",
			"frameworks": [],
			"providerShortName": null,
			"signingIdentity": null
		},
		"resources": [],
		"shortDescription": ""
	},
	"plugins": {},
	"app": {
		"windows": [
			{
				"fullscreen": false,
				"dragDropEnabled": false,
				"width": 1920,
				"height": 1080,
				"minWidth": 1280,
				"minHeight": 720,
				"resizable": true,
				"decorations": false,
				"transparent": true,
				"title": "Stagetune"
			}
		],
		"macOSPrivateApi": true,
		"security": {
			"assetProtocol": {
				"scope": ["**"],
				"enable": true
			},
			"csp": "default-src: 'self'; 'unsafe-inline'; media-src 'self' asset:; connect-src ipc: http://ipc.localhost"
		}
	}
}
